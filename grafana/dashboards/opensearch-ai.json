{
  "id": null,
  "title": "Backstage OpenSearch AI Plugin",
  "tags": ["backstage", "search", "ai", "opensearch"],
  "style": "dark",
  "timezone": "",
  "editable": true,
  "hideControls": false,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Search QPS",
      "gridPos": {"x": 0, "y": 0, "w": 4, "h": 4},
      "targets": [{
        "expr": "sum(rate(opensearch_ai_query_total{outcome=\"success\"}[5m]))",
        "legendFormat": "queries/sec",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 100}
            ]
          },
          "unit": "qps"
        }
      }
    },
    {
      "id": 2,
      "type": "stat", 
      "title": "Success Rate",
      "gridPos": {"x": 4, "y": 0, "w": 4, "h": 4},
      "targets": [{
        "expr": "sum(rate(opensearch_ai_query_total{outcome=\"success\"}[5m])) / sum(rate(opensearch_ai_query_total[5m])) * 100",
        "legendFormat": "success %",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 3,
      "type": "gauge",
      "title": "AI Fallback Rate",
      "gridPos": {"x": 8, "y": 0, "w": 4, "h": 4},
      "targets": [{
        "expr": "sum(rate(opensearch_ai_ai_usage_total{outcome=\"failure\"}[5m])) / clamp_min(sum(rate(opensearch_ai_ai_usage_total[5m])), 1e-9) * 100",
        "legendFormat": "fallback %",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 20}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Cache Hit Rate",
      "gridPos": {"x": 12, "y": 0, "w": 4, "h": 4},
      "targets": [{
        "expr": "sum(rate(opensearch_ai_cache_hits_total[5m])) / (sum(rate(opensearch_ai_cache_hits_total[5m])) + sum(rate(opensearch_ai_cache_misses_total[5m]))) * 100",
        "legendFormat": "cache %",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 80}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Query Latency Breakdown (p95)",
      "gridPos": {"x": 0, "y": 4, "w": 8, "h": 6},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(opensearch_ai_rewrite_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "Query Rewrite",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(opensearch_ai_search_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "OpenSearch",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(opensearch_ai_rerank_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "Re-ranking",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(opensearch_ai_embedding_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "Embeddings",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 0.1,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {"mode": "none", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {"type": "linear"},
            "axisCenteredZero": false,
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "thresholdsStyle": {"mode": "off"}
          }
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "AI Circuit Breaker Status",
      "gridPos": {"x": 8, "y": 4, "w": 8, "h": 6},
      "targets": [
        {
          "expr": "opensearch_ai_circuit_breaker_state{service=\"rewrite\"}",
          "legendFormat": "Query Rewrite CB",
          "refId": "A"
        },
        {
          "expr": "opensearch_ai_circuit_breaker_state{service=\"embedding\"}",
          "legendFormat": "Embedding CB", 
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2,
            "fillOpacity": 0.2,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "always",
            "pointSize": 8,
            "stacking": {"mode": "none", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "State (0=CLOSED, 1=OPEN, 2=HALF_OPEN)",
            "axisColorMode": "text",
            "scaleDistribution": {"type": "linear"},
            "axisCenteredZero": false,
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "thresholdsStyle": {"mode": "off"}
          },
          "mappings": [
            {"options": {"0": {"text": "CLOSED", "color": "green"}}, "type": "value"},
            {"options": {"1": {"text": "OPEN", "color": "red"}}, "type": "value"},
            {"options": {"2": {"text": "HALF_OPEN", "color": "yellow"}}, "type": "value"}
          ]
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Query Volume by Source",
      "gridPos": {"x": 0, "y": 10, "w": 8, "h": 6},
      "targets": [{
        "expr": "sum by (source) (rate(opensearch_ai_query_total[5m]))",
        "legendFormat": "{{source}}",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "qps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 0.1,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {"mode": "normal", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {"type": "linear"},
            "axisCenteredZero": false,
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "thresholdsStyle": {"mode": "off"}
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Indexing Throughput",
      "gridPos": {"x": 8, "y": 10, "w": 8, "h": 6},
      "targets": [{
        "expr": "sum by (source) (rate(opensearch_ai_indexed_total[5m]))",
        "legendFormat": "{{source}}",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 0.1,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {"mode": "normal", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {"type": "linear"},
            "axisCenteredZero": false,
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "thresholdsStyle": {"mode": "off"}
          }
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Error Rate by Type",
      "gridPos": {"x": 0, "y": 16, "w": 8, "h": 6},
      "targets": [
        {
          "expr": "sum by (error_type) (rate(opensearch_ai_errors_total[5m]))",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 0.1,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {"mode": "normal", "group": "A"},
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {"type": "linear"},
            "axisCenteredZero": false,
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "thresholdsStyle": {"mode": "off"}
          }
        }
      }
    },
    {
      "id": 10,
      "type": "table",
      "title": "Top Search Queries (Last Hour)",
      "gridPos": {"x": 8, "y": 16, "w": 8, "h": 6},
      "targets": [{
        "expr": "topk(10, sum by (query_hash) (increase(opensearch_ai_query_total[1h])))",
        "format": "table",
        "instant": true,
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "custom": {
            "align": "auto",
            "cellOptions": {"type": "auto"},
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null}
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {"Time": true, "__name__": true},
            "indexByName": {},
            "renameByName": {
              "query_hash": "Query Hash",
              "Value": "Count"
            }
          }
        }
      ]
    },
    {
      "id": 11,
      "type": "heatmap",
      "title": "Query Response Time Distribution",
      "gridPos": {"x": 0, "y": 22, "w": 16, "h": 8},
      "targets": [{
        "expr": "sum(rate(opensearch_ai_total_duration_seconds_bucket[5m])) by (le)",
        "format": "heatmap",
        "legendFormat": "{{le}}",
        "refId": "A"
      }],
      "options": {
        "calculate": false,
        "calculation": {},
        "cellGap": 1,
        "cellValues": {},
        "color": {
          "exponent": 0.5,
          "fill": "#b4ff00",
          "mode": "spectrum",
          "reverse": false,
          "scale": "exponential",
          "scheme": "Spectral",
          "steps": 64
        },
        "exemplars": {"color": "rgba(255,0,255,0.7)"},
        "filterValues": {"le": 1e-9},
        "legend": {"show": false},
        "rowsOptions": {"layout": "auto"},
        "tooltip": {"show": true, "yHistogram": false},
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false,
          "unit": "s"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "scaleDistribution": {"type": "linear"}
          }
        }
      }
    }
  ],
  "schemaVersion": 38,
  "version": 2,
  "uid": "opensearch-ai-dashboard",
  "description": "Comprehensive monitoring dashboard for Backstage OpenSearch AI Plugin performance and health metrics"
}
